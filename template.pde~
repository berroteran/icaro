#define PIC18F4550
void setup()
{
int i=0;
// pines de salida digital
for(i=0;i<8;i++)
pinMode(i,OUTPUT);
pinMode(8,INPUT);
pinMode(9,INPUT);
pinMode(25,INPUT);
pinMode(26,INPUT);
pinMode(27,INPUT);
pinMode(28,INPUT);
servo.attach(23);
servo.attach(24);
servo.attach(8);
servo.attach(9);
}

unsigned int sensor(int valor)
{
unsigned int an;
an=analogRead(valor);
return an;
}


void loop()
{
